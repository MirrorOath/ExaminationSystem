<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="dao.tables" auto-import="true">
    <class name="Score">
        <!-- 主键 ，映射-->
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="stuId" column="stuId" />
        <property name="jsTestId" column="jsTestId" />
        <property name="courseNo" column="courseNo" />
        <property name="courseName" column="courseName" />
        <property name="totalScore" column="totalScore" />
        <property name="score" column="score" />
        <property name="Pass" column="Pass" />
        <property name="ReTest" column="ReTest" />
        <property name="reTestMark" column="reTestMark" />
    </class>
</hibernate-mapping>
